/* fib.sd
 *
 * This test program computes the Nth Fibonacci number
 * using recursion and iteration.
 */

// global variables
int n = 8;

// iterative function
int itFibonacci(int n) {
  int Fn, FNminus1, temp;
  int b=0;
  Fn = 1;
  FNminus1 = 1;
  while (n > 2) {
    temp = Fn;
    Fn = Fn + FNminus1;
    FNminus1 = temp;
    n = n - 1;
  }
  return Fn;
}

/* main program */
void main()
{ 
  print "N: ";
  print n;
  print "Result of iterative computation:";
  print itFibonacci(n);
}